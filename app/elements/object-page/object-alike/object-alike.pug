.alike.section
  ._wrap.wrap
    h2._title Похожие объекты
    ._list
      ._item
        ._item-slide
          .swiper-container(data-attribute='sw__small')
            .swiper-wrapper
              .swiper-slide
                img(src="img/object_slide1.jpg", alt="slide")
              .swiper-slide
                img(src="img/object_slide2.jpg", alt="slide")
              .swiper-slide
                img(src="img/object_slide1.jpg", alt="slide")
              .swiper-slide
                img(src="img/object_slide3.jpg", alt="slide")
